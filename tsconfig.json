{
  "compilerOptions": {
    "downlevelIteration": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "esnext",
    "outDir": "./dist",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@constants": [
        "./src/utils/constants"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
    },
  },
}
